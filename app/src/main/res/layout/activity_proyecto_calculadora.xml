<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#FF43DFA3"
    android:padding="0dp"
    tools:context="unac.kapc.proyectocalculadora.ProyectoCalculadora">



    <TextView
        android:layout_width="fill_parent"
        android:layout_height="80dp"
        android:padding="0dp"
        android:background="#fbf7f7"
        android:id="@+id/textView"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/button10"
        android:layout_alignStart="@+id/button10" />


    <Button
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="1"
        android:id="@+id/button"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/button4"
        android:layout_alignStart="@+id/button4"
        android:textColor="#343333"
        android:backgroundTint="#43dfa3"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:textSize="30dp"
        android:layout_alignRight="@+id/button4"
        android:layout_alignEnd="@+id/button4" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="2"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/button"
        android:layout_toRightOf="@+id/button"
        android:backgroundTint="#FF43DFA3"
        android:textSize="30dp"
        android:textColor="#343333" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="3"
        android:id="@+id/button3"
        android:layout_alignTop="@+id/button2"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:backgroundTint="#FF43DFA3"
        android:textSize="30dp"
        android:textColor="#343333"
        android:layout_alignBottom="@+id/button2" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="4"
        android:id="@+id/button4"
        android:layout_below="@+id/button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:backgroundTint="#FF43DFA3"
        android:textSize="30dp"
        android:textColor="#343333"
        android:layout_alignBottom="@+id/button5" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="5"
        android:id="@+id/button5"
        android:layout_below="@+id/button"
        android:layout_toLeftOf="@+id/button3"
        android:layout_toStartOf="@+id/button3"
        android:layout_margin="0dp"
        android:backgroundTint="#FF43DFA3"
        android:textSize="30dp"
        android:textColor="#343333" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="6"
        android:id="@+id/button6"
        android:layout_below="@+id/button2"
        android:layout_alignRight="@+id/button3"
        android:layout_alignEnd="@+id/button3"
        android:backgroundTint="#FF43DFA3"
        android:textSize="30dp"
        android:textColor="#343333"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:layout_above="@+id/button8" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="7"
        android:id="@+id/button7"
        android:layout_toLeftOf="@+id/button5"
        android:layout_toStartOf="@+id/button5"
        android:backgroundTint="#FF43DFA3"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_below="@+id/button4"
        android:textSize="30dp"
        android:textColor="#343333" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="8"
        android:id="@+id/button8"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:backgroundTint="#FF43DFA3"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_below="@+id/button4"
        android:layout_above="@+id/button10"
        android:textSize="30dp"
        android:textColor="#343333" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="9"
        android:id="@+id/button9"
        android:layout_toRightOf="@+id/button5"
        android:layout_toEndOf="@+id/button5"
        android:backgroundTint="#FF43DFA3"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_below="@+id/button5"
        android:textSize="30dp"
        android:textColor="#343333"
        android:layout_alignBottom="@+id/button7" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="."
        android:id="@+id/button10"
        android:layout_toLeftOf="@+id/button8"
        android:layout_toStartOf="@+id/button8"
        android:backgroundTint="#FF43DFA3"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_above="@+id/textView"
        android:textSize="36dp"
        android:textColor="#343333"
        android:layout_below="@+id/button7" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="0"
        android:id="@+id/button11"
        android:layout_toLeftOf="@+id/button9"
        android:layout_toStartOf="@+id/button9"
        android:backgroundTint="#FF43DFA3"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_alignBottom="@+id/button10"
        android:textSize="30dp"
        android:textColor="#343333"
        android:layout_alignTop="@+id/button10" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="="
        android:id="@+id/button12"
        android:layout_alignTop="@+id/button11"
        android:layout_alignRight="@+id/button9"
        android:layout_alignEnd="@+id/button9"
        android:backgroundTint="#FF43DFA3"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:layout_above="@+id/textView"
        android:textSize="36dp"
        android:textColor="#343333" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="รท"
        android:id="@+id/button13"
        android:background="#343333"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:textSize="36dp"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/button3" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="x"
        android:id="@+id/button14"
        android:layout_alignLeft="@+id/button13"
        android:layout_alignStart="@+id/button13"
        android:layout_below="@+id/button3"
        android:background="#343333"
        android:textSize="26dp"
        android:textColor="#ffffff"
        android:layout_above="@+id/button9" />

    <Button
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:text="-"
        android:id="@+id/button15"
        android:layout_alignLeft="@+id/button14"
        android:layout_alignStart="@+id/button14"
        android:background="#343333"
        android:layout_below="@+id/button6"
        android:layout_above="@+id/button12"
        android:textSize="36dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="+"
        android:id="@+id/button16"
        android:layout_alignTop="@+id/button12"
        android:layout_alignRight="@+id/button15"
        android:layout_alignEnd="@+id/button15"
        android:background="#343333"
        android:layout_above="@+id/textView"
        android:textSize="36dp"
        android:textColor="#ffffff" />

</RelativeLayout>
